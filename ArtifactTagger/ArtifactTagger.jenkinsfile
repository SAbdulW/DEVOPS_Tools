pipeline {
    agent {
        node {
            label "Build-Slaves-B"
        }
    }
    options {
        buildDiscarder(logRotator(numToKeepStr: '10'))
        disableConcurrentBuilds()
    }
    /*
    triggers {
        cron('30 21 * * *')
    }*/

    stages {
        stage('Process'){
            steps{
                dir("./dashboard"){
                    bat "gradlew.bat run"
                }
            }
        }
    }
}